{
  "extends": ["@rocketseat/eslint-config/node"],
  "rules": {
    "no-useless-constructor": "off",
    "no-new": "off",
    "prettier/prettier": ["error", {
      "tabWidth": 2,
      "printWidth": 100,
      "singleQuote": true,
      "semi": false
      }],
      "@typescript-eslint/no-unused-vars": "warn"
  }
}
